<body>
<div class="all-wrapper menu-top">
  <div class="layout-w">

    <%= render 'layouts/header' %>

    <div class="content-w">
      <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="<%= admin_path %>">Home</a></li>
        <li class="breadcrumb-item"><a href="<%= admin_deal_uploads_path %>">Deal Uploads</a></li>
      </ul>

      <div class="content-i">
        <div class="content-box">
          <div class="element-wrapper">
            <h6 class="element-header">Deal Uploads</h6>

            <%= render 'layouts/flash' %>

            <div class="element-box">
              <h5 class="form-header">Deal Uploads List</h5>
              <div class="table-responsive">
                <table id="deal-uploads-table" width="100%" class="table table-striped table-lightfont" data-source="<%= admin_deal_uploads_path %>">
                  <thead>
                  <tr>
                    <th>ID</th>
                    <th>Image</th>
                    <th>User</th>
                    <th>Deal</th>
                    <th>Status</th>
                    <th>Created At</th>
                    <th>Actions</th>
                  </tr>
                  </thead>
                  <tbody>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="display-type"></div>
  <div id="modal"></div>
</div>
</body>