<body>
<div class="all-wrapper menu-top">
  <div class="layout-w">

    <%= render 'layouts/header' %>

    <div class="content-w">
      <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="<%= admin_path %>">Home</a></li>
        <li class="breadcrumb-item"><a href="<%= admin_hollow_users_path %>">Hollow Users</a></li>
      </ul>

      <div class="content-i">
        <div class="content-box">
          <div class="element-wrapper"><h6 class="element-header">Hollow Users</h6>

            <%= render 'layouts/flash' %>

            <div class="element-box">
              <h5 class="form-header">Hollow Users List</h5>
              <div class="table-responsive">
                <table id="hollow-users-table" width="100%" class="table table-striped table-lightfont" data-source="<%= admin_hollow_users_path %>">
                  <thead>
                  <tr>
                    <th>ID</th>
                    <th>Email</th>
                    <th>Mobile</th>
                    <th>Name</th>
                    <th>Refer ID</th>
                    <th>Sponsor ID</th>
                    <th>Real Sponsor ID</th>
                    <th>Created At</th>
                  </tr>
                  </thead>
                  <tbody>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="display-type"></div>
</div>
</body>