<body>
<div class="all-wrapper menu-top">
  <div class="layout-w">

    <%= render 'layouts/header' %>

    <div class="content-w">
      <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="<%= admin_path %>">Home</a></li>
        <li class="breadcrumb-item"><a href="<%= admin_quiz_winners_path %>">Quiz Winners</a></li>
      </ul>

      <div class="content-i">
        <div class="content-box">
          <div class="element-wrapper"><h6 class="element-header">Quiz Winners</h6>

            <%= render 'layouts/flash' %>

            <div class="element-box">
              <h5 class="form-header">Quiz Winners List</h5>
              <div class="table-responsive">
                <table id="quiz-winners-table" width="100%" class="table table-striped table-lightfont" data-source="<%= admin_quiz_winners_path %>">
                  <thead>
                  <tr>
                    <th>ID</th>
                    <th>Quiz</th>
                    <th>User</th>
                    <th>Points</th>
                    <th>Position</th>
                    <th>Created At</th>
                  </tr>
                  </thead>
                  <tbody>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="display-type"></div>
</div>
</body>